        },
        {
            "name": "Levi Morrison",
            "email": "levim@php.net",
            "homepage": "http://morrisonlevi.github.com/",
            "role": "Developer"
        },
        {
            "name": "Dan Ackroyd",
            "email": "Danack@basereality.com",
            "homepage": "http://www.basereality.com",
            "role": "Developer"
        },
        {
            "name": "Martin Hughes",
            "email": "martin-hughes@users.noreply.github.com",
            "homepage": "https://github.com/martin-hughes",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=7.4.0 <8.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Auryn\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Auryn\\Test\\": "test/"
        },
        "files": ["test/fixtures.php"]
    }
}
